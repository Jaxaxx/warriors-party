<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mine.warriorsservermultisource.mapper.second.SecondSysUserMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.mine.warriorsservermultisource.entity.SysUser">
        <id column="t_id" property="tId"/>
        <result column="create_date" property="createDate"/>
        <result column="t_name" property="tName"/>
        <result column="update_date" property="updateDate"/>
        <result column="version" property="version"/>
        <result column="delete_flag" property="deleteFlag"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        t_id, create_date, t_name, update_date, version, delete_flag
    </sql>

    <select id="queryAll" resultMap="BaseResultMap">
        select <include refid="Base_Column_List" /> FROM sys_user
    </select>


</mapper>
